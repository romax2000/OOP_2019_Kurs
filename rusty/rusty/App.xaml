<Application x:Class="rusty.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:rusty"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Style/DataGrid.xaml"/>
                <ResourceDictionary Source="Resources/Style/Button.xaml"/>
                <ResourceDictionary Source="Resources/Style/CustomMessageBox/ButtonStyle.xaml"/>
                <ResourceDictionary Source="Resources/Style/CustomMessageBox/TextBoxStyle.xaml"/>
                <ResourceDictionary Source="Resources/Style/Label.xaml"/>
                <ResourceDictionary Source="Resources/Style/TextBox.xaml"/>
                <ResourceDictionary Source="Resources/Style/ComboBox.xaml"/>
                <ResourceDictionary Source="Resources/Style/CustomMessageBox/WindowStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
